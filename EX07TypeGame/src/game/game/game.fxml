<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.game.GameController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <center>
            <Label fx:id="label2" text="2. TÄHT" BorderPane.alignment="CENTER" />
         </center>
         <left>
            <Label fx:id="label1" alignment="CENTER" contentDisplay="CENTER" text="1. TÄHT" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Label fx:id="label3" alignment="CENTER" text="3. TÄHT" BorderPane.alignment="CENTER" />
         </right>
         <top>
            <Label fx:id="scoreText" text="SCORE" textAlignment="CENTER" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <Button fx:id="backToMenu" mnemonicParsing="false" onAction="#returnToFront" text="Button" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
